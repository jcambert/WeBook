@page "/"

@namespace WeBook.services.SignalR.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>WeBook</title>
    <base href="~/" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&amp;display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,700&display=swap" />
    <link id="westrap-theme" rel="stylesheet" href="_content/WeStrap/css/bootstrap.css" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css" rel="stylesheet" type='text/css'>

    <script defer src="_content/WeStrap/js/westrap.js"></script>
    <script defer src="_content/WeStrap/js/wesignalr.js"></script>
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    <app>
        @(await Html.RenderComponentAsync<App>(RenderMode.ServerPrerendered))
    </app>

    <script src="_framework/blazor.server.js"></script>
    <script src="_content/WeQuery/wequery.js"></script>
    <script src="~/js/aspnet-signalr/signalr.js"></script>
</body>
</html>
